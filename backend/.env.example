# Environment variables for Agriculture Insights Platform
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# JWT Configuration
JWT_SECRET_KEY=your_jwt_secret_key_here_change_in_production

# Development Settings
DEBUG=True
HOST=0.0.0.0
PORT=8000

# Database Settings (if using PostgreSQL directly)
DATABASE_URL=postgresql://username:password@localhost/agriculture_insights

# External API Keys (optional)
WEATHER_API_KEY=your_weather_api_key_here
SATELLITE_API_KEY=your_satellite_data_api_key_here

# Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here

# Production Settings
ENVIRONMENT=development